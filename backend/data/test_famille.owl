        <se_marier_avec rdf:resource="#Manel"/>
        <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">40</age>
        <nationalite rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tunisienne</nationalite>
        <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Omar</nom>
