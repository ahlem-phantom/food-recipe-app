                <se_marier_avec rdf:resource="#Mohamed"/>
                <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">69</age>
                <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aycha</nom>
              </Femme>
            </se_marier_avec>
            <nationalite rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tunisienne</nationalite>
            <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mohamed</nom>
            <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">70</age>
          </Homme>
        </estFilsDe>
      </Homme>
    </se_marier_avec>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">25</age>
  </Femme>
  <Femme rdf:ID="Sonia">
