    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Fille">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="estFilleDe"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Enfant"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Fils"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Soeur">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="estSoeurDe"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="#Femme"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Fraterie"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Pere">
