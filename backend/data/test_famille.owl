      </Homme>
    </se_marier_avec>
    <nationalite rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tunisienne</nationalite>
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sonia</nom>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">30</age>
  </Femme>
  <Homme rdf:ID="Ali">
    <estFilsDe rdf:resource="#Mohamed"/>
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ali</nom>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">38</age>
  </Homme>
  <Femme rdf:ID="Ameni">
    <estFilleDe rdf:resource="#Sonia"/>
