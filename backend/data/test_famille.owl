    <nationalite rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tunisienne</nationalite>
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ameni</nom>
  </Femme>
  <Femme rdf:ID="Fatima">
    <estFilleDe rdf:resource="#Mohamed"/>
    <estFilleDe rdf:resource="#Aycha"/>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">18</age>
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fatima</nom>
  </Femme>
  <Homme rdf:ID="Jamel">
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jamel</nom>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">5</age>
    <estFilsDe rdf:resource="#Manel"/>
    <estFilsDe rdf:resource="#Omar"/>
  </Homme>
  <Homme rdf:ID="Zied">
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Zied</nom>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">10</age>
    <estFilsDe rdf:resource="#Manel"/>
    <estFilsDe rdf:resource="#Omar"/>
  </Homme>
  <Homme rdf:ID="Nader">
    <nationalite rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marocaine</nationalite>
    <estFilsDe rdf:resource="#Khaled"/>
    <age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">10</age>
    <nom rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nader</nom>
  </Homme>
